<rcvBufCfg>
	 <!-- number of frames each receive buffer -->
    	<nFramePerRcvBuf>1</nFramePerRcvBuf>
	<deviceName1>0000:84:00.0</deviceName1>
	<deviceName2>0000:84:00.1</deviceName2>

	<filePath>/media/DATA-STORE/</filePath>
	
	 <!-- filename for storing Model data -->
	<ifStoreReferenceImage>true</ifStoreReferenceImage>
	<referencePreName>refer</referencePreName>

        <!-- filename for storing received data -->
	<ifStoreResultImage>true</ifStoreResultImage>
    	<imagePreName>res</imagePreName>

	<ifStoreROI>true</ifStoreROI>
	<roiPreName>roi</roiPreName>
	
	<serialPort>/dev/ttyS1</serialPort>

       	

        <pauseTime>120</pauseTime>

  	<!-- number of receive buffers each receive pool -->
    	<nRcvBufPerPool>1600</nRcvBufPerPool>

	<roiChartWidth>500</roiChartWidth>

	<indexStartX>0</indexStartX>
	<indexEndX>2047</indexEndX>
	<indexStartY>0</indexStartY>
	<indexEndY>2047</indexEndY>
	
	<pointDensity>80</pointDensity>

	<regNumber>24</regNumber>

	<referenceNumber>240</referenceNumber>


	<roiGroupNumber>20</roiGroupNumber>
	
	<normalNumber>30</normalNumber>
	<normalPercent>0.2</normalPercent>	

	<ifFeedback>false</ifFeedback>

	<filterNumber>200</filterNumber>

	<varianceThreshold>0.002</varianceThreshold>

	<meanThreshold>0.04</meanThreshold>
	<ROIFixed>90</ROIFixed>
	
       
</rcvBufCfg>
